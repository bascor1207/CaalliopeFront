{
    "settings": {
        "import/extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
        ],
        "import/parsers": {
            "@typescript-eslint/parser": [
            ".ts",
            ".tsx"
            ]
    },
    "import/resolver": {
        "node": {
        "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
        ],
        "paths": [
            "src"
        ]
        }
    }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "react-hooks",
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/eslint-recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "getter-return": "error",
        "no-duplicate-imports": "warn",
        "no-dupe-class-members": "warn",
        "no-dupe-else-if": "error",
        "no-setter-return": "error",
        "no-this-before-super": "warn",
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            1,
            {
              "extensions": [
                ".js",
                ".jsx",
                "ts",
                "tsx"
              ]
            }
          ],
          "indent": [
            "error",
            4,
            {
              "SwitchCase": 1,
              "ignoredNodes": [
                "TemplateLiteral"
              ]
            }
          ],
          "camelcase": 0,
          "react/prop-types": 0,
          "require-jsdoc": 0,
          "no-multiple-empty-lines": [
            "error",
            {
              "max": 1
            }
          ],
          "quote-props": [
            "error",
            "as-needed"
          ],
          "import/no-named-as-default": 0,
          "quotes": [
            2,
            "single",
            "avoid-escape"
          ],
          "no-case-declarations": 0,
          "eol-last": [
            "warn",
            "always"
          ],
          "no-extra-parens": "off",
          "no-multi-spaces": "error",
          "semi": [
            2,
            "always"
          ],
          "no-unused-vars": [
            1
          ],
          "no-var": "error",
          "one-var": [
            "error",
            {
              "let": "never",
              "const": "never"
            }
        ],
        "jsx-quotes": [
            "warn",
            "prefer-double"
        ],
        "react/jsx-equals-spacing": [
            "warn",
            "never"
        ]
    }
}